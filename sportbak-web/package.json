{
  "name": "finticks-demo",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development",
    "start_external": "ng serve -c development --open --host 0.0.0.0 --disable-host-check ",
    "start_ssl" : "ng serve -c development --ssl true --ssl-key ./node_modules/browser-sync/certs/server.key --ssl-cert ./node_modules/browser-sync/certs/server.crt",
    "serve_out": "ng serve --host=0.0.0.0 --disable-host-check",
    "build": "ng build -c production --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_patch": "npm version patch --commit-hooks=false --git-tag-version=false && npm run build",
    "build_minor": "npm version minor --commit-hooks=false --git-tag-version=false && npm run build",
    "build_major": "npm version major --commit-hooks=false --git-tag-version=false && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@googlemaps/js-api-loader": "^1.11.3",
    "@googlemaps/markerclustererplus": "^1.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/google-maps": "^3.2.2",
    "@types/googlemaps": "^3.43.3",
    "aos": "^2.3.4",
    "chart.js": "2.9.3",
    "gts": "^3.1.0",
    "jquery": "^3.5.1",
    "ng-click-outside": "^8.0.0",
    "ng-in-viewport": "^6.1.5",
    "ng2-charts": "^2.4.2",
    "ngx-slick-carousel": "^0.5.1",
    "rxjs": "~7.1.0",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "~12.0.3",
    "@angular/language-service": "~12.0.3",
    "@types/google.maps": "^3.44.3",
    "@types/jasmine": "~3.7.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.5",
    "@types/node": "~15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "browser-sync": "^2.27.5",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng2-charts-schematics": "^0.1.7",
    "protractor": "~7.0.0",
    "ts-node": "~10.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.2.3"
